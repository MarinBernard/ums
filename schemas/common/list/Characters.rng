<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
	<define name="LT_Characters">
		<element name="characters">
			<oneOrMore>
				<choice>
					<ref name="CT_Character"/>
					<ref name="RT_Character"/>
				</choice>
			</oneOrMore>
		</element>
	</define>
</grammar>